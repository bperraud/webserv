[
	{
	  "listen": ["0.0.0.0", 8080],
	  "max_body_size": 200000,
	  "error_pages": {
		"404": "www/404.html",
		"405": "www/405.html"
	  },
	  "routes": {
		"/": {
		  "root": "www",
		  "index": "index.html",
		  "methods": ["GET"]
		},
		"/form/add": {
		  "root": "www",
		  "handler": "www/cgi/add.py",
		  "methods": ["GET"],
		  "cgi": {
			".py": "/usr/bin/python3"
		  }
		},
		"/form/sub": {
			"root": "www",
			"handler": "www/cgi/sub.php",
			"methods": ["GET"],
			"cgi": {
			  ".php": "/usr/local/opt/php@8.0/bin/php"
			}
		  },
		"/form/upload": {
		  "root": "www/upload",
		  "methods": ["POST"]
		},
		"/upload": {
		  "root": "www/upload",
		  "methods": ["GET", "DELETE"],
		  "autoindex": true
		},
		"/sendback": {
			"root": "www/sendback",
			"methods": ["POST"]
		},
		"/hello": {
		  "root": "www/hello",
		  "autoindex": true,
		  "methods": ["GET"],
		  "cgi": {
			".py": "/usr/bin/python3",
			".php": "/usr/bin/php-cgi",
			".js": "/usr/local/bin/node",
			".rb": "/usr/bin/ruby",
			".sh": "/bin/sh",
			".lua": "/usr/bin/lua"
		  }
		},
		"/redir": {
			"root": "www/redir",
			"methods": ["GET"],
			"redir": "https://www.pechetruite.com/"
		},
		"/cookies": {
		  "root": "www",
		  "methods": ["GET"],
		  "handler": "www/cgi/cookies.py",
		  "cgi": {
			".py": "/usr/bin/python3"
		  }
		}
	  }
	}
]
